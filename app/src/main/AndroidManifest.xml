<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wrig.truehb_ranchi_app_v1">

    <uses-feature
        android:name="android.hardware.usb.host"
        android:required="true" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:requestLegacyExternalStorage="true">


        <activity android:name=".views.activities.admin.DataDisplayActivity"></activity>
        <activity android:name=".views.activities.admin.DataFilterActivity" />
        <activity android:name=".views.activities.FinalTestActivity" />
        <activity android:name=".views.activities.DeviceCheckActivity" />
        <activity android:name=".views.activities.TestResportActivity" />
        <activity android:name=".views.activities.ClientDetailsInputActivity" />
        <activity android:name=".views.activities.admin.SuperAdminDashBaordActivity" />
        <activity android:name=".views.activities.UserDashBoardActivity" />
        <activity android:name=".views.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activities.MainActivity" />

        <service
            android:name=".services.UsbService"
            android:enabled="true" />
    </application>

</manifest>